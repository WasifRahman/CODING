{
   "python.testing.pytestArgs": [
      "Anisul YT"
   ],
   "python.testing.unittestEnabled": false,
   "python.testing.nosetestsEnabled": false,
   "python.testing.pytestEnabled": false,
   "python.testing.promptToConfigure": false,
   "python.pythonPath": "C:\\Users\\User\\AppData\\Local\\Microsoft\\WindowsApps\\python.exe",
   "cSpell.ignoreWords": [
      "isnumeric"
   ],
   "workbench.colorCustomizations": {

      "terminal.foreground": "#05a373",
      "terminalCursor.foreground": "#026581"
      
      
   },
   "cSpell.words": [
      "foodlist",
      "GETOUT",
      "Momos",
      "Ogmanted",
      "Wipped"
   ],
   "terminal.integrated.drawBoldTextInBrightColors": true,
   "terminal.integrated.tabs.defaultColor": "terminal.ansiBlue",
   "scss.lint.argumentsInColorFunction": "error",
   "cSpell.customDictionaries": {
      "custom-dictionary-workspace": {
         "name": "custom-dictionary-workspace",
         "path": "${workspaceFolder:Python}/.cspell/custom-dictionary-workspace.txt",
         "addWords": true,
         "scope": "workspace"
      }
   }
}